{
  "name": "zedme",
  "displayName": "zedme",
  "description": "zed atom beatiful style with recommended Zed font",
  "version": "0.2.10",
  "publisher": "ironbeast",
  "icon": "favicon.png",
"repository": {
    "type": "git",
    "url": "https://github.com/lornally/vsce.git"
  },
  "homepage": "isuyu.cn",
  "engines": {
    "vscode": "^1.99.0"
  },
  "categories": [
    "Themes"
  ],
  "contributes": {
    "grammars": [
      {
        "path": "./syntaxes/python_syntax.json",
        "scopeName": "source.python.export.injection",
        "injectTo": [
          "source.python"
        ]
      }
    ],
    "themes": [
      {
        "label": "zedme",
        "uiTheme": "vs-dark",
        "path": "./themes/zem.json"
      }
    ],
    "commands": [
      {
        "command": "zedme.setRecommendedFont",
        "title": "Zedme: 设置推荐字体"
      }
    ]
  },
  "activationEvents": [],
  "main": "./extension.js"
}